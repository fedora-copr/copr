[backend]
results_baseurl=https://{{ ansible_host }}/results
frontend_base_url=https://{{ ansible_host }}
frontend_auth={{ be_fe_token["content"] | b64decode }}
destdir=/var/lib/copr/public_html/results
statsdir=/var/lib/copr/public_html/stats
do_sign=true
keygen_host=localhost:8080

[ssh]
builder_config=/home/copr/.ssh/config
